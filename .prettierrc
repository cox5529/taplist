{
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "printWidth": 120,
  "bracketSpacing": true,
  "jsxSingleQuote": true,
  "importOrderSeparation": true,
  "importOrder": [
    "^react$",
    "^[^\\.]*$",
    "^((\\./)|(\\.\\./))+[^\\.]+$",
    "^\\..*.css$",
    "^(\\./)[^\\.]+.*$",
    "^.*$"
  ],
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
